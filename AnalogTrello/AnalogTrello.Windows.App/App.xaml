<Application
    x:Class="AnalogTrello.Windows.App.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:app="clr-namespace:AnalogTrello.Windows.App"
    xmlns:core="clr-namespace:Core"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <app:Bootstraper x:Key="Bootstrapper" />
                </ResourceDictionary>
                <ui:ThemesDictionary Theme="Light" />
                <ui:ControlsDictionary />


                <ResourceDictionary Source="Resource/Style/Color.xaml" />
                <ResourceDictionary Source="Resource/Style/NavBarStyle.xaml" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
